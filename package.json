{
  "name": "@createz/contracts",
  "version": "0.0.1",
  "description": "A creator contribution system",
  "keywords": [
    "solidty",
    "truffle"
  ],
  "author": "suiiii",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suiiii-rip/createz-contracts.git"
  },
  "bugs": {
    "url": "https://github.com/suiiii-rip/createz-contracts/issues"
  },
  "homepage": "https://createz.xyz",
  "types": "./types/web3-v1-contracts/types.d.ts",
  "scripts": {
    "generate-types": "typechain --target=truffle-v5 'build/contracts/**.json'",
    "generate-frontend-types": "typechain --target=web3-v1 'build/contracts/**.json'",
    "build": "truffle compile && npm run generate-types && npm run generate-frontend-types",
    "clean": "rimraf types/ build/",
    "test": "truffle test"
  },
  "devDependencies": {
    "@typechain/truffle-v5": "^8.0.0",
    "@typechain/web3-v1": "^6.0.0",
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.24",
    "rimraf": "^3.0.2",
    "truffle": "^5.5.10",
    "ts-node": "^10.7.0",
    "typechain": "^8.0.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.5.0",
    "solidity-linked-list": "^5.1.0"
  }
}
