{
  "name": "createz",
  "version": "0.1.0",
  "description": "A creator contribution system",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "sass",
    "hmr",
    "solidty",
    "truffle"
  ],
  "author": "suiiii",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suiiii-rip/createz.git"
  },
  "bugs": {
    "url": "https://github.com/suiiii-rip/createz/issues"
  },
  "homepage": "https://createz.xyz",
  "scripts": {
    "build": "NODE_ENV=production npm run clean-dist && webpack --config=app/configs/webpack/prod.js",
    "generate-types": "typechain --target=truffle-v5 'build/contracts/**.json'",
    "truffle-compile": "truffle compile && npm run generate-types",
    "clean-dist": "rimraf dist/*",
    "lint": "eslint './app/src/**/*.{js,ts,tsx}' --quiet",
    "start": "NODE_ENV=development npm run start-dev",
    "start-dev": "webpack serve --config=app/configs/webpack/dev.js",
    "start-prod": "NODE_ENV=production npm run build && node express.js",
    "test": "NODE_ENV=test nyc --reporter=text ts-mocha --require ./app/tests/setup.js --require ignore-styles ./app/tests/**/*.test.{js,jsx,ts,tsx} --exit",
    "test-watch": "NODE_ENV=test nyc --reporter=text ts-mocha --require ./app/tests/setup.js --require ignore-styles ./app/tests/**/*.test.{js,jsx,ts,tsx} --exit --watch --watch-files ./app/tests/**",
    "truffle-test": "truffle test"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@openzeppelin/contracts": "^4.0.0",
    "@typechain/truffle-v5": "^4.0.1",
    "@types/bn.js": "^4.11.6",
    "@types/chai": "^4.2.16",
    "@types/enzyme": "^3.10.8",
    "@types/jsdom": "^16.2.10",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.34",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/styled-components": "^5.1.9",
    "@types/web3": "^1.2.2",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "css-loader": "^5.1.2",
    "enzyme": "^3.11.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "global-jsdom": "^8.1.0",
    "html-webpack-plugin": "^5.3.1",
    "ignore-styles": "^5.0.1",
    "image-webpack-loader": "^7.0.1",
    "jsdom": "^16.5.3",
    "mocha": "^8.2.2",
    "node-polyfill-webpack-plugin": "^1.1.0",
    "node-sass": "^5.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^11.0.1",
    "solidity-linked-list": "^5.0.0",
    "style-loader": "^2.0.0",
    "truffle": "^5.3.0",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.1.1",
    "typechain": "^4.0.3",
    "typescript": "^4.2.3",
    "webpack": "^5.25.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hot-loader": "^4.13.0",
    "styled-components": "^5.2.3",
    "web3": "^1.2.9"
  }
}
