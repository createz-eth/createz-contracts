{
  "name": "createz",
  "version": "0.1.0",
  "description": "A creator contribution system",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "sass",
    "hmr",
    "solidty",
    "truffle"
  ],
  "author": "suiiii",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suiiii-rip/createz.git"
  },
  "bugs": {
    "url": "https://github.com/suiiii-rip/createz/issues"
  },
  "homepage": "https://createz.xyz",
  "scripts": {
    "build": "npm run clean-dist && webpack --config=app/configs/webpack/prod.js",
    "truffle-compile": "truffle compile",
    "clean-dist": "rimraf dist/*",
    "lint": "eslint './app/src/**/*.{js,ts,tsx}' --quiet",
    "start": "npm run start-dev",
    "start-dev": "webpack serve --config=app/configs/webpack/dev.js",
    "start-prod": "npm run build && node express.js",
    "test": "jest --coverage --config=app/configs/jest.json",
    "test-watch": "jest --coverage --watchAll --config=app/configs/jest.json",
    "truffle-test": "truffle test"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@openzeppelin/contracts": "^4.0.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.34",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.1.2",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "image-webpack-loader": "^7.0.1",
    "jest": "^26.6.3",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hot-loader": "^4.13.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "truffle": "^5.3.0",
    "typescript": "^4.2.3",
    "webpack": "^5.25.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
